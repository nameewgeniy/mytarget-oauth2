{
    "name": "nameewgeniy/mytarget-oauth2",
    "type": "library",
    "description": "MyTarget oauth2 api library",
    "keywords": [
        "nameewgeniy",
        "myTarget",
		"api",
        "oauth2",
        "token"
    ],
    "homepage": "https://github.com/nameewgeniy/mytarget-oauth2",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "nameewgeniy",
            "email": "nameewgeniy@mail.ru",
            "homepage": "https://github.com/nameewgeniy",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "nameewgeniy\\myTarget\\api\\oauth2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "nameewgeniy\\myTarget\\api\\oauth2\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./codecept run unit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
